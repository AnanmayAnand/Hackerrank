#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int total,days,hours,minutes,seconds;
    scanf("%d",&total);
    days=total/(24*60*60);
    hours=(total%(24*60*60))/(60*60);
    minutes=total%(60*60)/60;
    seconds=total%60;
    printf("The Duration is %d days ",days);
    printf("%d hours ",hours);
    printf("%d minutes ",minutes);
    printf("%d seconds",seconds);
   
    return 0;
}
